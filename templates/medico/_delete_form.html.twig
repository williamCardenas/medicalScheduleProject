<form method="post" action="{{ path('medico_delete', {'id': medico.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ medico.id) }}">
    <button class="btn btn-danger">Delete</button>
</form>