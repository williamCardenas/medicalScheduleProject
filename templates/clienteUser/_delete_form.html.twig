<form method="post" action="{{ path('cliente_user_delete', {'id': user.id,'clienteId': cliente.id}) }}" onsubmit="return confirm('{% trans with {'_entidade':'teste'} %}mensagem.deletar{% endtrans %}');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn">Delete</button>
</form>
